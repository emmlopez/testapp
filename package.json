{
  "name": "testapp",
  "private": false,
  "version": "0.0.1",
  "description": "test cx 5",
  "author": "emmanuel",
  "main": "src/index",
  "scripts": {
    "clean:install": "rimraf node_modules && rm pnpm-lock.yaml && pnpm install",
    "remove-demo": "npm-run-all --parallel removeDemo:*",
    "removeDemo:Examples": "node buildTools/removeExampleDemo.js && rimraf buildTools/removeExampleDemo.js",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "start:prod": "http-server -p 5001 --proxy \"http://localhost:5001?\" ./build",
    "analyze": "npm run build -- --analyze",
    "lint": "eslint --ext .js,.jsx,.tsx,.ts src/",
    "lint:fix": "eslint --ext .js,.jsx,.tsx,.ts src/ --fix",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run --spec \"cypress/integration/*/*.spec.js\"",
    "cy:local": "concurrently \"pnpm run start\" \"pnpm run cy:open\" -k -s first",
    "cy:CI": "concurrently \"pnpm run start\" \"pnpm run cypress:test\" -k -s first",
    "cypress:test": "wait-on http://localhost:3000 && pnpm run cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emmlopez/testapp"
  },
  "engine": {
    "node": ">=14"
  },
  "keywords": [],
